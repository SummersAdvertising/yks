<h1>Listing Ticket</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tickets.each do |ticket| %>
  <tr>
    <td><%= ticket.title %></td>
    <td><%= show ticket %></td>
    <td><%= edit ticket %></td>
    <td><%= delete ticket %></td>
  </tr>
<% end %>
</table>

