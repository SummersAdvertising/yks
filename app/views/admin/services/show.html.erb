<h1><%= t("services.tablename") %> - <%= t("helpers.show") %></h1>

<%= render 'shared/admin_form_notice' %>
<div class="show">
<div>
  <b><%= t("services.title") %>：</b>
  <%= @service.title %>
</div>

<div>
  <b><%= t("services.time") %>：</b>
  <%= @service.time %>
</div>

<div>
  <b><%= t("services.content") %>：</b>
  <%= simple_format @service.content %>
</div>

<%= render 'shared/admin_form_command', :data => @service %>
</div>