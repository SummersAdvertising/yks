<h1><%= t("user_exps.tablename") %> - <%= t("helpers.show") %></h1>

<%= render 'shared/admin_form_notice' %>
<div class="show">
<div>
  <b><%= t("user_exps.title") %>：</b>
  <%= @userexp.title %>
</div>

<div>
  <b><%= t("user_exps.name") %>：</b>
  <%= @userexp.name %>
</div>

<div>
  <b><%= t("user_exps.time") %>：</b>
  <%= @userexp.time %>
</div>

<div>
  <b><%= t("user_exps.content") %>：</b>
  <%= simple_format @userexp.content %>
</div>

<%= render 'shared/admin_form_command', :data => @userexp %>
</div>
