<h1><%= t("services.tablename") %><%= new %></h1>


<div class="list">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr class="list-title">
    <td><%= t("services.title") %></td>
  </tr>

<% @services.each do |service| %>
  <tr class="list-content">
    <td class="first"><%= link_to service.title, :action => 'show', :id => service.parameter %>
		<div class="list-command">
		<%= link_to t("helpers.edit"), {:action => "edit", :id => service.parameter}, :class => "mod" %><span>â”‚</span>
		<%= link_to t("helpers.delete"), { :action => "destroy", :id => service.parameter }, :class => "del", :confirm => "Are you sure you want to delete this article?", :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
</div>