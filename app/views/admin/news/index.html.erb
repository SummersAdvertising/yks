<h1><%= t("news.tablename") %><%= new %></h1>


<div class="list">
<%= render :partial => 'shared/pagination'%>

<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr class="list-title">
    <td><%= t("news.title") %></td>
    <td><%= t("news.time") %></td>
  </tr>

<% @news.each do |new| %>
  <tr class="list-content">
    <td class="first"><a><%= show new, new.title %></a><%= render 'shared/admin_list_command', :data => new %></td>
    <td><%= new.time %></td>
  </tr>
<% end %>
</table>
</div>
