<h1><%= t("photos.tablename") %><%= new %></h1>


<div class="list">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr class="list-title">
    <td width="20%"></td>
    <td><%= t("photos.name") %></td>
  </tr>

<% @photos.each do |photo| %>
  <tr class="list-content">
  	<td class="first" align="center" valign="middle"><img src="<%= photo.filename %>" alt="open" /></td>
    <td class="first"><a><%= show photo, photo.name %></a>
    	<div class="list-command"><%= link_to '修改', :action => :edit, :id =>  photo.id %></div>
    </td>
  </tr>
<% end %>
</table>
</div>
