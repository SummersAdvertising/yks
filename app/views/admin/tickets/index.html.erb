<h1><%= t("tickets.tablename") %><%= new %></h1>


<div class="list">
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr class="list-title">
    <td><%= t("tickets.title") %></td>
    <td><%= t("tickettypes.time") %></td>
    <td><%= t("tickettypes.status") %></td>
  </tr>

<% @tickets.each do |ticket| %>
  <tr class="list-content">
    <td class="first"><a><%= show ticket, ticket.title %></a><%= render 'shared/admin_list_command', :data => ticket %></td>
    <td><%= ticket.time %></td>
    <td><%= ticket.define.name %></td>
  </tr>
<% end %>
</table>
</div>